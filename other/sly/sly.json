  "config": {
    "icons": {
      "directory": "./other/svg-icons",
      "postinstall": [
        "npm",
        "run",
        "build:icons"
      ],
      "transformers": [
        "transform-icon.ts"
      ]
    },
    "components": {},
    "utils": {}
  },
  "libraries": {
    "iconify:radix-icons": {
      "name": "Radix Icons",
      "config": "icons"
    },
    "iconify:lucide": {
      "name": "Lucide Icons",
      "config": "icons"
    },
    "iconify:heroicons": {
      "config": "icons"
    }
  }
}